{
  "name": "@coveo/angular",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "test": "npm run build && jasmine dist/**/*_spec.js",
    "build": "tsc -p tsconfig.json",
    "BROKENcopy:collection": "cp src/collection.json dist/coveo-angular/collection.json",
    "BROKENcopy:schemas": "cp src/coveo-angular/schema.json dist/coveo-angular/",
    "BROKENcopy:files": "cp --parents -p src/*/files/** ../../dist/coveo-angular/",
    "postbuildBROKEN": "npm run copy:schemas && npm run copy:files && npm run copy:collection",
    "test:schematics": "npm run build && schematics .:ng-add --orgId=zxc --apiKey=\"xc345-ds\""
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^11.1.2",
    "@angular-devkit/schematics": "^11.1.2",
    "@angular/cdk": "^11.1.1",
    "@angular/cli": "^11.1.2",
    "@schematics/angular": "^11.1.2",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0"
  }
}
