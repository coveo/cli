{
  "name": "@coveo/angular",
  "version": "1.27.1",
  "description": "Coveo schematic for angular",
  "scripts": {
    "test": "jest",
    "lint": "prettier --check . && eslint .",
    "build": "tsc -p tsconfig.json",
    "prebuild": "node ./scripts/clean.js",
    "postbuild": "node ./scripts/setup.js",
    "prepublishOnly": "npm run build",
    "npm:bump:template": "npm --no-git-tag-version version 0.0.0",
    "npm:publish:template:local": "npm publish --access public --registry http://verdaccio:4873",
    "npm:publish:template": "npm publish --access public --registry http://localhost:4873",
    "release:phase2": "node --experimental-specifier-resolution=node ../../scripts/releaseV2/phase2-bump-all-packages.mjs"
  },
  "keywords": [
    "schematics",
    "coveo",
    "headless",
    "angular"
  ],
  "author": "Coveo",
  "main": "index.js",
  "license": "Apache-2.0",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^13.2.4",
    "@angular-devkit/schematics": "^13.2.4",
    "@angular/cdk": "^13.2.3",
    "@angular/cli": "^13.2.4",
    "@coveo/search-token-server": "1.27.1",
    "@schematics/angular": "^13.2.4",
    "jest-config": "^27.4.7",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@angular/core": "^13.2.3",
    "@angular/forms": "^13.2.3",
    "@angular/material": "^13.2.3",
    "@angular/platform-browser": "^13.2.3",
    "@angular/router": "^13.2.3",
    "@coveo/headless": "latest",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.29",
    "fs-extra": "10.1.0",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
