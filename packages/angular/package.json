{
  "name": "@coveo/angular",
  "version": "1.0.1",
  "description": "Coveo schematic for angular",
  "scripts": {
    "test": "npm run build && jasmine dist/**/*_spec.js",
    "lint": "prettier --check . && eslint .",
    "build": "tsc -p tsconfig.json",
    "prebuild": "node ./scripts/clean.js",
    "postbuild": "node ./scripts/setup.js",
    "prepublishOnly": "npm run build",
    "npm:bump:template": "npm --no-git-tag-version version",
    "npm:publish:template": "npm publish --access public --tag ci-test"
  },
  "keywords": [
    "schematics",
    "coveo",
    "headless",
    "angular"
  ],
  "author": "Coveo",
  "main": "index.js",
  "license": "Apache-2.0",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^11.1.2",
    "@angular-devkit/schematics": "^11.1.2",
    "@angular/cdk": "^11.1.1",
    "@angular/cli": "^11.1.2",
    "@coveo/search-token-server": "^1.0.0",
    "@schematics/angular": "^11.1.2",
    "typescript": "~4.1.2"
  },
  "devDependencies": {
    "@angular/core": "^11.2.2",
    "@angular/forms": "^11.2.2",
    "@angular/material": "^11.2.1",
    "@angular/router": "^11.1.2",
    "@coveo/headless": "latest",
    "@types/fs-extra": "^9.0.9",
    "@types/jasmine": "~3.5.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^12.11.1",
    "fs-extra": "^9.1.0",
    "jasmine": "^3.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
