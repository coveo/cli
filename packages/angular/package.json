{
  "name": "@coveo/angular",
  "version": "1.24.1",
  "description": "Coveo schematic for angular",
  "scripts": {
    "test": "npm run build && jasmine dist/**/*_spec.js",
    "lint": "prettier --check . && eslint .",
    "build": "tsc -p tsconfig.json",
    "prebuild": "node ./scripts/clean.js",
    "postbuild": "node ./scripts/setup.js",
    "prepublishOnly": "npm run build",
    "npm:bump:template": "npm --no-git-tag-version version",
    "npm:publish:template:local": "npm publish --access public --registry http://verdaccio:4873",
    "npm:publish:template": "npm publish --access public --registry http://localhost:4873"
  },
  "keywords": [
    "schematics",
    "coveo",
    "headless",
    "angular"
  ],
  "author": "Coveo",
  "main": "index.js",
  "license": "Apache-2.0",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "13.2.1",
    "@angular-devkit/schematics": "13.2.1",
    "@angular/cdk": "13.2.0",
    "@angular/cli": "13.2.1",
    "@coveo/search-token-server": "latest",
    "@schematics/angular": "13.2.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@angular/core": "13.2.0",
    "@angular/forms": "13.2.0",
    "@angular/material": "13.2.0",
    "@angular/platform-browser": "13.2.0",
    "@angular/router": "13.2.0",
    "@coveo/headless": "latest",
    "@types/fs-extra": "9.0.13",
    "@types/jasmine": "3.10.3",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.10",
    "fs-extra": "10.0.0",
    "jasmine": "4.0.2",
    "prettier": "2.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
