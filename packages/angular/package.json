{
  "name": "@coveo/angular",
  "version": "1.8.0",
  "description": "Coveo schematic for angular",
  "scripts": {
    "test": "npm run build && jasmine dist/**/*_spec.js",
    "lint": "prettier --check . && eslint .",
    "build": "tsc -p tsconfig.json",
    "prebuild": "node ./scripts/clean.js",
    "postbuild": "node ./scripts/setup.js",
    "prepublishOnly": "npm run build",
    "npm:bump:template": "npm --no-git-tag-version version",
    "npm:publish:template": "npm publish --access public"
  },
  "keywords": [
    "schematics",
    "coveo",
    "headless",
    "angular"
  ],
  "author": "Coveo",
  "main": "index.js",
  "license": "Apache-2.0",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.1.1",
    "@angular-devkit/schematics": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/cli": "^12.1.1",
    "@coveo/search-token-server": "latest",
    "@schematics/angular": "^12.1.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@angular/core": "12.1.1",
    "@angular/forms": "12.1.1",
    "@angular/material": "12.1.1",
    "@angular/router": "12.1.1",
    "@coveo/headless": "latest",
    "@types/fs-extra": "9.0.11",
    "@types/jasmine": "3.5.14",
    "@types/mocha": "8.2.2",
    "@types/node": "14.17.4",
    "fs-extra": "10.0.0",
    "jasmine": "3.7.0",
    "prettier": "2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
