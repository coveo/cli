{
  "name": "@coveo/angular",
  "version": "1.25.0",
  "description": "Coveo schematic for angular",
  "scripts": {
    "snyk:prepare": "cp ../../package-lock.json ./package-lock.json && npm i",
    "test": "jest",
    "lint": "prettier --check . && eslint .",
    "build": "tsc -p tsconfig.json",
    "prebuild": "node ./scripts/clean.js",
    "postbuild": "node ./scripts/setup.js",
    "prepublishOnly": "npm run build",
    "npm:bump:template": "npm --no-git-tag-version version 0.0.0",
    "npm:publish:template:local": "npm publish --access public --registry http://verdaccio:4873",
    "npm:publish:template": "npm publish --access public --registry http://localhost:4873",
    "release:phase2": "node --experimental-specifier-resolution=node ../../scripts/releaseV2/phase2-bump-all-packages.mjs"
  },
  "keywords": [
    "schematics",
    "coveo",
    "headless",
    "angular"
  ],
  "author": "Coveo",
  "main": "index.js",
  "license": "Apache-2.0",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "13.2.0",
    "@angular-devkit/schematics": "13.2.0",
    "@angular/cdk": "13.2.0",
    "@angular/cli": "13.2.0",
    "@coveo/search-token-server": "1.25.0",
    "@schematics/angular": "13.2.0",
    "jest-config": "^27.4.7",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@angular/core": "13.2.0",
    "@angular/forms": "13.2.0",
    "@angular/material": "13.2.0",
    "@angular/router": "13.2.0",
    "@coveo/headless": "latest",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.12",
    "fs-extra": "10.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
