{
  "name": "@coveo/create-atomic",
  "description": "Coveo Atomic Generator",
  "version": "1.36.3",
  "files": [
    "/lib",
    "/template",
    "/template/.gitignore"
  ],
  "bin": "./index.mjs",
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "start": "node ./index.mjs",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "rimraf template && npm run build && node ./scripts/preparePackageJsonTemplate.mjs",
    "lint": "prettier --check . && eslint .",
    "release:phase1": "node --experimental-specifier-resolution=node ../../../../scripts/releaseV2/phase1-bump-all-packages.mjs"
  },
  "keywords": [
    "coveo",
    "headless",
    "atomic",
    "template",
    "generator"
  ],
  "author": "Coveo",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@coveo/platform-client": "37.13.0",
    "abortcontroller-polyfill": "1.7.5",
    "detect-indent": "7.0.1",
    "https-proxy-agent": "5.0.1",
    "inquirer": "9.1.4",
    "isomorphic-fetch": "3.0.0",
    "minimist": "1.2.7",
    "node-plop": "^0.31.0",
    "plop": "3.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "fs-extra": "10.1.0"
  }
}
