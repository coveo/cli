    "jest-config": "29.5.0"
  },
  "devDependencies": {
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@coveo/headless": "latest",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.3",
    "fs-extra": "11.1.1",
