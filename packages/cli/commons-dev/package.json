{
  "name": "@coveo/cli-commons-dev",
  "version": "4.0.0",
  "author": "Coveo",
  "description": "Common utils for @coveo/cli plugins development",
  "license": "Apache-2.0",
  "files": [
    "lib/**",
    ".eslintrc.js",
    ".prettierrc.js",
    "tsconfig.json"
  ],
  "exports": {
    "./.eslintrc": "./.eslintrc.js",
    "./.eslintrc.js": "./.eslintrc.js",
    "./.prettierrc": "./.prettierrc.js",
    "./.prettierrc.js": "./.prettierrc.js",
    "./tsconfig.json": "./tsconfig.json",
    "./*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "dependencies": {
    "fancy-test": "2.0.7",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@types/jest": "29.2.2",
    "typescript": "4.8.4"
  },
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "prepublish": "npm run build",
    "release:phase1": "node --experimental-specifier-resolution=node ../../../scripts/releaseV2/phase1-bump-all-packages.mjs"
  },
  "publishConfig": {
    "access": "public"
  }
}
