{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "experimentalDecorators": true,
    "declaration": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "module": "Node16",
    "allowSyntheticDefaultImports": true,
    "lib": ["ESNext"],
    "resolveJsonModule": true,
    "strict": true,
    "target": "es2019",
    "noEmitOnError": false,
    "types": ["node", "jest"],
    "esModuleInterop": true
  }
}
