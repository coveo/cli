{
  "name": "@coveo/create-atomic",
  "description": "Coveo Atomic Generator",
  "version": "1.24.1",
  "files": [
    "/lib",
    "/templates",
    "/templates/.gitignore"
  ],
  "bin": {
    "create-atomic": "./index.js"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "snyk:prepare": "cp ../../package-lock.json ./package-lock.json && npm i && node --loader ts-node/esm ./script/npmInstallTemplate.ts",
    "start": "./index.js",
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "npm:bump:template": "npm --no-git-tag-version version 0.0.0",
    "npm:publish:template": "npm publish --access public --registry http://localhost:4873",
    "npm:publish:template:local": "npm publish --access public --registry http://verdaccio:4873",
    "lint": "prettier --check . && eslint .",
    "release:phase2": "node --experimental-specifier-resolution=node ../../scripts/releaseV2/phase2-bump-all-packages.js",
    "release:phase4": "node --experimental-specifier-resolution=node ../../scripts/releaseV2/phase4-npm-publish-packages.js"
  },
  "keywords": [
    "coveo",
    "headless",
    "atomic",
    "template",
    "generator"
  ],
  "author": "Coveo",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "minimist": "1.2.5",
    "plop": "^3.0.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  }
}