{
  "name": "@coveo/create-atomic",
  "private": true,
  "description": "Coveo Atomic Generator",
  "version": "1.20.0",
  "files": [
    "/lib",
    "/templates"
  ],
  "bin": {
    "create-atomic": "./index.mjs"
  },
  "main": "index.mjs",
  "scripts": {
    "start": "plop --plopfile ./lib/plopfile.js",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "notyet:npm:bump:template": "npm --no-git-tag-version version",
    "notyet:npm:publish:template": "npm publish --access public --registry http://localhost:4873",
    "lint": "prettier --check . && eslint ."
  },
  "keywords": [
    "coveo",
    "headless",
    "atomic",
    "template",
    "generator"
  ],
  "author": "Coveo",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "minimist": "1.2.5",
    "plop": "2.7.6",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  }
}
