{
  "name": "@coveo/create-atomic",
  "description": "Coveo Atomic Generator",
  "version": "1.21.3",
  "files": [
    "/lib",
    "/templates",
    "/templates/.gitignore"
  ],
  "bin": {
    "create-atomic": "./index.js"
  },
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "postinstall": "node --loader ts-node/esm ./script/npmInstallTemplate.ts",
    "start": "./index.js",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "npm:bump:template": "npm --no-git-tag-version version",
    "npm:publish:template": "npm publish --access public --registry http://localhost:4873",
    "lint": "prettier --check . && eslint ."
  },
  "keywords": [
    "coveo",
    "headless",
    "atomic",
    "template",
    "generator"
  ],
  "author": "Coveo",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "minimist": "1.2.5",
    "plop": "^3.0.1",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
}
