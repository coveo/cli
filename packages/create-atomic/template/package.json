{
  "name": "@coveo/create-atomic-template",
  "description": "{{titleCase project}} project",
  "version": "1.25.4",
  "private": true,
  "scripts": {
    "setup-lambda": "node ./scripts/setup-lambda.js",
    "setup-cleanup": "node ./scripts/clean-up.js",
    "prestart": "fkill -s :3333 :8888",
    "start": "netlify dev",
    "build": "stencil build",
    "dev:live": "npm run site:link && npm run prestart && netlify dev --live",
