{
  "markup": "<atomic-search-interface>\n  <atomic-search-layout>\n    <div class=\"logo\">\n        <img src=\"https://www.coveo.com/public/img/logos/coveo/coveo_logo_en.svg\" alt=\"logo\"/>\n    </div>\n  \n    <atomic-layout-section section=\"search\">\n  <atomic-search-box></atomic-search-box>\n</atomic-layout-section>\n\n    <atomic-layout-section section=\"facets\">\n    <atomic-facet-manager><atomic-facet field=\"source\" label=\"Source\" display-values-as=\"checkbox\"></atomic-facet><atomic-facet field=\"filetype\" label=\"Filetype\" display-values-as=\"checkbox\"></atomic-facet>\n    </atomic-facet-manager></atomic-layout-section>\n\n    <atomic-layout-section section=\"main\">\n      <atomic-layout-section section=\"status\">\n  <atomic-breadbox></atomic-breadbox>\n  <atomic-query-summary></atomic-query-summary>\n  <atomic-refine-toggle></atomic-refine-toggle>\n  <atomic-sort-dropdown><atomic-sort-expression label=\"Relevance\" expression=\"relevancy\"></atomic-sort-expression><atomic-sort-expression label=\"Most recent\" expression=\"date descending\"></atomic-sort-expression></atomic-sort-dropdown>\n  <atomic-did-you-mean></atomic-did-you-mean>\n</atomic-layout-section>\n\n      <atomic-layout-section section=\"results\">\n  --results--\n  <atomic-query-error></atomic-query-error>\n  <atomic-no-results></atomic-no-results>\n</atomic-layout-section>\n\n      <atomic-layout-section section=\"pagination\">\n  <atomic-load-more-results></atomic-load-more-results>\n</atomic-layout-section>\n\n    </atomic-layout-section>\n  </atomic-search-layout>\n</atomic-search-interface>\n",
