    "searchInterface": "<atomic-search-interface id=\"search\">\n  <atomic-search-layout>\n    <div class=\"logo\">\n          <img src=\"https://www.coveo.com/public/img/logos/coveo/coveo_logo_en.svg\" alt=\"logo\"/>\n        </div>\n      <atomic-layout-section section=\"search\">\n      <atomic-search-box class=\"search-box-item\"></atomic-search-box>\n    </atomic-layout-section>\n    <atomic-layout-section section=\"facets\">\n      <atomic-facet-manager><atomic-facet field=\"source\" label=\"Source\" display-values-as=\"checkbox\"/><atomic-facet field=\"filetype\" label=\"Filetype\" display-values-as=\"checkbox\"/></atomic-facet-manager>\n    </atomic-layout-section>\n    <atomic-layout-section section=\"main\">\n      <atomic-layout-section section=\"status\">\n        <atomic-breadbox></atomic-breadbox>\n        <atomic-query-summary></atomic-query-summary>\n        <atomic-refine-toggle></atomic-refine-toggle>\n        <atomic-sort-dropdown><atomic-sort-expression label=\"Relevance\" expression=\"relevancy\"/><atomic-sort-expression label=\"Most recent\" expression=\"date descending\"/></atomic-sort-dropdown>\n        <atomic-did-you-mean></atomic-did-you-mean>\n      </atomic-layout-section>\n      <atomic-layout-section section=\"results\">\n        <!--result-list-->\n        <atomic-query-error></atomic-query-error>\n        <atomic-no-results></atomic-no-results>\n      </atomic-layout-section>\n      <atomic-layout-section section=\"pagination\">\n        <atomic-load-more-results></atomic-load-more-results>\n      </atomic-layout-section>\n    </atomic-layout-section>\n  </atomic-search-layout>\n</atomic-search-interface>",
    "style": ":root {\n    --atomic-primary: ;\n    --atomic-primary-light: ;\n    --atomic-primary-dark: ;\n    --atomic-on-background: ;\n}\n\nbody {\n    margin: 0;\n}\n\natomic-search-layout {\n    padding-top: 5rem;\n    position: relative;\n}\n\n.logo {\n    position: absolute;\n    display: grid;\n    align-self: center;\n    align-items: center;\n    justify-items: center;\n    height: 4rem;\n    top: 0;\n    left: 0;\n    right: 0;\n    border-bottom: 1px solid var(--atomic-neutral);\n}\n\n.logo img {\n    display: none;\n    max-height: 2rem;\n}\n\natomic-search-interface.hydrated .logo img {\n    display: block;\n}\n\n@media only screen and (min-width: 1024px) {\n    atomic-search-layout {\n        padding-top: 0;\n    }\n\n    .logo {\n        position: unset;\n        justify-items: start;\n        grid-area: 1 / 2;\n        height: 5rem;\n        border-bottom: none;\n    }\n\n    .logo img {\n        max-height: 3rem;\n    }\n}\n",
