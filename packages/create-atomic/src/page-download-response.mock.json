    "layout": "body {\n  margin: 0;\n}\n\natomic-search-layout {\n  position: relative;\n  height: auto;\n}\n\n.logo + atomic-layout-section[section='search'] {\n  padding-top: 5rem;\n}\n\n.logo {\n  position: absolute;\n  display: grid;\n  align-self: center;\n  align-items: center;\n  justify-items: center;\n  height: 4rem;\n  top: 0;\n  left: 0;\n  right: 0;\n  border-bottom: 1px solid var(--atomic-neutral);\n}\n\n.logo img {\n  display: none;\n  max-height: 2rem;\n}\n\natomic-search-interface.hydrated .logo img {\n  display: block;\n}\n\n@media only screen and (min-width: 1024px) {\n  .logo + atomic-layout-section[section='search'] {\n    padding-top: 0;\n  }\n\n  .logo {\n    position: unset;\n    justify-items: start;\n    grid-area: 1 / 2;\n    height: 5rem;\n    border-bottom: none;\n  }\n\n  .logo img {\n    max-height: 3rem;\n  }\n}\n\n@media only screen and (min-width: 599px) {\n  .coveo-styleguide .modal-backdrop .modal-container .modal-content {\n    min-width: 600px;\n    max-width: 750px;\n  }\n  .configuration-modal #configuration-modal {\n    width: 85%;\n    height: 90%;\n    max-width: none;\n    min-width: 1024px;\n  }\n}"
