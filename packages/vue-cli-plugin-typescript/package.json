{
  "name": "@coveo/vue-cli-plugin-typescript",
  "version": "1.25.0",
  "description": "Vue-cli 3 plugin for adding Coveo Headless support",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coveo/cli.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "snyk:prepare": "cp ../../package-lock.json ./package-lock.json && npm i",
    "lint": "prettier --check . && eslint .",
    "npm:bump:template": "npm --no-git-tag-version version 0.0.0",
    "npm:publish:template": "npm publish --access public --registry http://localhost:4873",
    "npm:publish:template:local": "npm publish --access public --registry http://verdaccio:4873",
    "release:phase2": "node --experimental-specifier-resolution=node ../../scripts/releaseV2/phase2-bump-all-packages.mjs",
    "release:phase4": "node --experimental-specifier-resolution=node ../../scripts/releaseV2/phase4-npm-publish-packages.mjs"
  },
  "devDependencies": {
    "@coveo/headless": "1.42.0",
    "buefy": "0.9.14",
    "dotenv": "14.3.2",
    "vue": "2.6.14",
    "vue-property-decorator": "9.1.2",
    "vue-router": "3.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
