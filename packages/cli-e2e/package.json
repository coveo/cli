    "@babel/core": "7.12.17",
    "@babel/preset-env": "7.12.17",
    "@babel/preset-typescript": "7.12.17",
    "async-retry": "1.3.1",
    "axios": "^0.21.1",
    "babel-jest": "26.6.3",
    "fs-extra": "^9.1.0",
    "jest": "26.6.3",
