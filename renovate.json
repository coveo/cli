{
  "enabled": true,
  "extends": [
    ":semanticPrefixFixDepsChoreOthers",
    "helpers:pinGitHubActionDigests"
  ],
  "schedule": ["before 2am"],
  "vulnerabilityAlerts": {
    "commitMessageSuffix": "[SECURITY] J:CDX-227"
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "groupName": "all dependencies",
      "groupSlug": "all"
    },
    {
      "matchPackagePatterns": ["^@angular/*", "^@angular-devkit/*"],
      "rangeStrategy": "replace"
    },
    {
      "matchPackageNames": ["strip-ansi"],
      "allowedVersions": "6.x"
    },
    {
      "matchPackageNames": ["get-port"],
      "allowedVersions": "5.x"
    },
    {
      "matchPackageNames": ["chalk"],
      "allowedVersions": "4.x"
    },
    {
      "matchPackageNames": ["vue"],
      "allowedVersions": "2.x",
      "description": "Need to support Vue3 TODO:CDX-813"
    },
    {
      "matchPackageNames": ["vue-router"],
      "allowedVersions": "3.x",
      "description": "Need to support Vue3 TODO:CDX-813"
    },
    {
      "matchPackageNames": ["fkill"],
      "matchCurrentVersion": "7.x",
      "allowedVersions": "7.x",
      "description": "Need to migrate E2E to ESM (not easy)"
    },
    {
      "matchPackageNames": ["jest", "babel-jest", "jest-config", "ts-jest"],
      "matchCurrentVersion": "27.x",
      "allowedVersions": "27.x",
      "description": "CDX-942"
    }
  ],
  "rangeStrategy": "auto",
  "lockFileMaintenance": {
    "enabled": true
  },
  "commitMessageSuffix": "J:CDX-227"
}
