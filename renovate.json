{
  "enabled": true,
  "extends": [
    ":semanticPrefixFixDepsChoreOthers",
    "helpers:pinGitHubActionDigests"
  ],
  "schedule": ["before 2am"],
  "vulnerabilityAlerts": {
    "commitMessageSuffix": "[SECURITY] J:CDX-227"
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "groupName": "all dependencies",
      "groupSlug": "all"
    },
    {
      "matchPackagePatterns": ["^@angular/*", "^@angular-devkit/*"],
      "rangeStrategy": "replace"
    },
    {
      "matchPackageNames": ["strip-ansi"],
      "allowedVersions": "6.x"
    },
    {
      "matchPackageNames": ["get-port"],
      "allowedVersions": "5.x"
    },
    {
      "matchPackageNames": ["chalk"],
      "allowedVersions": "4.x"
    },
    {
      "matchPackageNames": ["@oclif/plugin-update"],
      "allowedVersions": "2.x",
      "description": "Wait for Oclif 2 TODO:CDX-849"
    },
    {
      "matchPackageNames": ["npm"],
      "allowedVersions": "8.4.x",
      "description": "Need NPM to be fixed https://github.com/npm/cli/issues/4404 TODO:CDX-815"
    },
    {
      "matchPackageNames": ["vue"],
      "allowedVersions": "2.x",
      "description": "Need to support Vue3 TODO:CDX-813"
    },
    {
      "matchPackageNames": ["vue-router"],
      "allowedVersions": "3.x",
      "description": "Need to support Vue3 TODO:CDX-813"
    },
    {
      "matchPackageNames": ["fkill"],
      "matchCurrentVersion": "7.x",
      "allowedVersions": "7.x",
      "description": "Need to migrate E2E to ESM (not easy)"
    },
    {
      "matchPackageNames": ["react", "react-dom"],
      "allowedVersions": "17.x",
      "description": "Need Material UI support for React 18. TODO:CDX-897"
    }
  ],
  "rangeStrategy": "auto",
  "lockFileMaintenance": {
    "enabled": true
  },
  "commitMessageSuffix": "J:CDX-227"
}
