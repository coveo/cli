{
  "deployment_config_version": 19,
  "product": "cli",
  "team_name": "DX",
  "general": {
    "team_jenkins": "searchuibuilds",
    "notifications": {
      "slack_channels": [
        "#searchuibuilds"
      ]
    },
    "environments_order": {
      "sequential": [
        "dev",
        "stg",
        {
          "concurrent": [
            "prd",
            "hip"
          ]
        }
      ]
    },
    "aws_regions": {
      "sequential": [
        "FIRST_MAIN_REGION"
      ]
    },
    "overrides": [
      {
        "environments": [
          "stg",
          "prd",
          "hip"
        ],
        "content": {
          "start_environment_automatically": true
        }
      }
    ]
  },
  "ordered_phases": [
    {
      "id": "terraform",
      "terraform": {}
    },
    {
      "id": "s3",
      "s3": {
        "bucket": "{terraform.infra.infra.bucket_binaries}",
        "directory": "proda/StaticCDN/cli",
        "source": "./artifacts",
        "parameters": {
          "acl": "public-read"
        },
        "overrides": [
          {
            "environments": [
              "hip"
            ],
            "content": {
              "disabled": true
            }
          }
        ]
      }
    },
    {
      "id": "invalidate_cache",
      "team_jenkins": {
        "job_name": "cli-cloudfront-invalidation",
        "disabled": true,
        "overrides": [
          {
            "environments": [
              "prd"
            ],
            "content": {
              "disabled": false
            }
          }
        ]
      }
    }
  ],
  "certifiers": {
    "observatory": {
      "no_endpoint": true
    },
    "package_rollout": {
      "only_consider_changesets_after": "591040c6568cce03b031c3059cd4d4de87de8d7c",
      "jira_issues_discovery": {
        "stop_after_first_issue": true,
        "branch_name": false,
        "commit_message": false,
        "pull_request_title": true,
        "pull_request_description": true
      }
    }
  }
}